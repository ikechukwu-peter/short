{
  "name": "nip",
  "version": "1.0.0",
  "description": "A nodejs application to shorten and expand url",
  "main": "server.ts",
  "scripts": {
    "start": "tsc && node dist/server.js",
    "dev": "nodemon --exec npx ts-node src/server.ts",
    "watch:js": "esbuild --watch --bundle  ./public/js/index.ts --outfile=./public/js/bundle.js",
    "build": "tsc -p .",
    "build:js": "esbuild --minify --bundle  ./public/js/index.ts --outfile=./public/js/bundle.js"
  },

  "repository": "https:github.com/ikechukwu-peter/nip.git",
  "author": "Ikechukwu Peter Chinedu",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.24.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "esbuild": "^0.14.7",
    "express": "^4.17.2",
    "mongoose": "^6.1.2",
    "parcel": "^2.0.1",
    "pug": "^3.0.2",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.1",
    "@types/request": "^2.48.7",
    "dotenv": "^10.0.0",
    "nodemon": "^2.0.15",
    "poi": "^12.10.3",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.4"
  }
}
